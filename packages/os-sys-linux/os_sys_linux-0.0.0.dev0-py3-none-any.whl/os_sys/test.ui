<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="Frame">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.Button" id="file">
        <property name="command">file</property>
        <property name="compound">bottom</property>
        <property name="default">normal</property>
        <property name="overrelief">sunken</property>
        <property name="relief">raised</property>
        <property name="text" translatable="yes">add file</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="target">
        <property name="command">select</property>
        <property name="text" translatable="yes">target file</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">ne</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="folder">
        <property name="command">folder</property>
        <property name="cursor">arrow</property>
        <property name="font">TkDefaultFont</property>
        <property name="justify">left</property>
        <property name="text" translatable="yes">add folder</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Canvas" id="Canvas_3">
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="files">
        <property name="command">files</property>
        <property name="default">active</property>
        <property name="font">TkDefaultFont</property>
        <property name="overrelief">groove</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">add files</property>
        <bind add="" handler="callback" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="save">
        <property name="command">close</property>
        <property name="text" translatable="yes">save</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="extract">
        <property name="command">read</property>
        <property name="cursor">arrow</property>
        <property name="font">TkDefaultFont</property>
        <property name="text" translatable="yes">extract wheel file</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="conver zip to whl">
        <property name="anchor">n</property>
        <property name="command">con</property>
        <property name="text" translatable="yes">conver zip to whl</property>
        <bind add="" handler="callback" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="convert whl to zip">
        <property name="command">conz</property>
        <property name="justify">left</property>
        <property name="text" translatable="yes">convert whl to zip</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">se</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Message" id="Message_1">
        <property name="font">TkFixedFont</property>
        <property name="text" translatable="yes">first you need to select a file. you do that when you press the button target file. then you can choise add an folder, files or one file. if you have choise everything you want you just press the save button to close the file
</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Sizegrip" id="Sizegrip_1">
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">10</property>
          <property name="sticky">se</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
