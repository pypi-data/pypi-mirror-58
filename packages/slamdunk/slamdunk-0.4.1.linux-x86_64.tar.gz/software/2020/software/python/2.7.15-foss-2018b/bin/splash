#!/software/2020/software/python/2.7.15-foss-2018b/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'slamdunk==0.4.1','console_scripts','splash'
__requires__ = 'slamdunk==0.4.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('slamdunk==0.4.1', 'console_scripts', 'splash')()
    )
