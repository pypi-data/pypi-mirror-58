#!/usr/local/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'divik==2.3.7b0','console_scripts','spectral'
__requires__ = 'divik==2.3.7b0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('divik==2.3.7b0', 'console_scripts', 'spectral')()
    )
