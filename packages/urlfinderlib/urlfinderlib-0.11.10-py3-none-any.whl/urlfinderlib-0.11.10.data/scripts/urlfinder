#!python

import urlfinderlib
import sys

if __name__ == '__main__':
    try:
        file_path = sys.argv[1]
    except:
        print('Usage: urlfinder /path/to/file')
        sys.exit(1)

    try:
        with open(file_path, 'rb') as f:
            urls = sorted(list(set(urlfinderlib.find_urls(f.read()))))
            for url in urls:
                print(url)
    except:
        print('Usage: urlfinder /path/to/file')
        sys.exit(1)
