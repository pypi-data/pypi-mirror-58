#ifndef bioExprNormalCdf_h
#define bioExprNormalCdf_h



class bioExpression ;


class bioExprNormalCdf: public bioGhFunction {
 public:
  bioExprNormalCdf(bioExpression* e, std::vector<bioUInt> derivl, bioUInt l, bioBoolean wg, bioBoolean wh) ;
  std::vector<bioReal> getValue(bioReal x) ;
  bioUInt getSize() const ;
private:
  bioBoolean withGradient ;
  bioBoolean withHessian ;
  bioExpression* theExpression ;
  std::vector<bioUInt> derivLiteralIds ;
  bioUInt rvId;
  bioReal theValue ;
};

#endif
